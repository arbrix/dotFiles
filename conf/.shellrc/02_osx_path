# Homebrew #####################################################################
# Add homebrew to the PATH
if [ $(uname) = "Darwin" ]; then
  export HOMEBREW=/opt/homebrew
  if [[ -d $HOMEBREW ]]; then
    export PATH=$HOMEBREW/bin:$HOMEBREW/sbin:$HOMEBREW/gettext/bin:$PATH
    export MANPATH=$HOMEBREW/share/man:$MANPATH
  fi
fi
